# ========================================
# 多智能体系统环境变量配置模板
# ========================================

# --- 日志配置 ---
LOG_LEVEL=INFO

# ========================================
# 模型配置 - 每个模型独立的环境变量
# ========================================

# --- Qwen Plus 模型配置 ---
QWEN_PLUS_API_KEY=your_qwen_plus_api_key_here
QWEN_PLUS_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
QWEN_PLUS_MODEL=qwen-plus-latest
QWEN_PLUS_PRICE_PROMPT=01EN_PLUS_PRICE_COMPLETION=002

# --- DeepSeek V3 模型配置 ---
DEEPSEEK_V3_API_KEY=your_deepseek_v3_api_key_here
DEEPSEEK_V3BASE_URL=http://61.49535:302v1
DEEPSEEK_V3_MODEL=deepseek-v3
DEEPSEEK_V3_PRICE_PROMPT=0.1
DEEPSEEK_V3_PRICE_COMPLETION=002

# --- DeepSeek R1 模型配置 ---
DEEPSEEK_R1_API_KEY=your_deepseek_r1_api_key_here
DEEPSEEK_R1BASE_URL=http://61.49535:301v1
DEEPSEEK_R1_MODEL=deepseek-r1
DEEPSEEK_R1_PRICE_PROMPT=0.1
DEEPSEEK_R1_PRICE_COMPLETION=00.002

# --- OpenAI GPT 模型配置 ---
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_MODEL=gpt-4OPENAI_PRICE_PROMPT=0.03
OPENAI_PRICE_COMPLETION=006
# --- Claude 模型配置 ---
CLAUDE_API_KEY=your_claude_api_key_here
CLAUDE_BASE_URL=https://api.anthropic.com/v1
CLAUDE_MODEL=claude-3sonnet-20240229CLAUDE_PRICE_PROMPT=0.015
CLAUDE_PRICE_COMPLETION=0.075

# ========================================
# 外部API配置
# ========================================

# --- Bochai API 配置 ---
BOCHAI_API_URL=your_bochai_api_url_here
BOCHAI_API_KEY=your_bochai_api_key_here

# --- SearXNG 搜索配置 ---
SEARXNG_API_URL=your_searxng_api_url_here

# --- Vision API 配置 ---
VISION_API_URL=your_vision_api_url_here

# ========================================
# 系统配置
# ========================================

# 默认模型选择 (可选值: qwen-plus, deepseek-v3, deepseek-r1, openai, claude)
DEFAULT_MODEL=qwen-plus

# 模型轮询策略 (可选值: round_robin, random, cost_optimized)
MODEL_SELECTION_STRATEGY=cost_optimized

# 最大并发请求数
MAX_CONCURRENT_REQUESTS=5

# 请求超时时间(秒)
REQUEST_TIMEOUT=30

# 重试次数
MAX_RETRIES=3 